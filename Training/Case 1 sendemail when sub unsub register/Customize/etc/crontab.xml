<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="mpcustomize_cronjob_customer_register_success_send_email" instance="Mageplaza\Customize\Cron\RegisterSuccessCron" method="execute">
            <config_path>mp_customize/cron_job/cron_schedule</config_path>
        </job>
        <job name="mpcustomize_cronjob_subscription_send_email" instance="Mageplaza\Customize\Cron\SubscriptionCron" method="execute">
            <config_path>mp_customize/cron_job/cron_schedule</config_path>
        </job>
    </group>
</config>
